{
  "extends": [
    "./node_modules/gts/",
    "plugin:storybook/recommended"
  ],
  "rules": {
    "prettier/prettier": "off",
    "n/no-extraneous-import": ["error", {
      "allowModules": ["@storybook/react"]
    }]
  },
  "overrides": [
    {
      "files": ["*.stories.tsx"],
      "rules": {
        "n/no-extraneous-import": "off"
      }
    }
  ]
}
